<div class="container">
  <div class="login-card card-panel teal col darken-text center-align">
    <h2 class="page-title white-text">Create Account</h2>
    <form class="form signup-form">
      <div class="form-group">
        <label for="name-signup" class="white-text">Name:</label>
        <input class="form-input center-align" type="text" id="name-signup" />
      </div>
      <div class="form-group">
        <label for="email-signup" class="white-text">Email:</label>
        <input class="form-input center-align" type="text" id="email-signup" />
      </div>
      <div class="form-group">
        <label for="password-signup" class="white-text">Password:</label>
        <input
          class="form-input center-align"
          type="password"
          id="password-signup"
          autocomplete="on"
        />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">signup</button>
      </div>
    </form>
    <br />
    <p class="white-text">Already have an account? sign in
      <a href="login" class="orange-text darken-3">here!</a></p>
  </div>
</div>
<script src="./js/signup.js"></script>