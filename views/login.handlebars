<div class="row justify-space-between-md">
  <div class="login-card card-panel teal col darken-text">
    <h2 class="page-title white-text">Sign in</h2>
    <form class="form login-form">
      <div class="form-group">
        <label for="email-login " class="white-text">Email:</label>
        <input class="form-input" type="text" id="email-login" />
      </div>
      <div class="form-group">
        <label for="password-login" class="white-text">Password:</label>
        <input class="form-input" type="password" id="password-login" />
      </div>
      <div class="form-group">
        <button class="btn btn-primary" type="submit">Login</button>
      </div>
    </form>
    <br />
    <p class="white-text">Don't have an account? Create one
      <a href="signup" class="orange-text darken-3">here!</a></p>
  </div>
</div>
<script src="./js/login.js"></script>