<div class="container carousel-container z-depth-3">
  <div class="carousel">

    {{#each allBooks as |book|}}
      <a class="carousel-item" href="#">
        <img class="z-depth-5" src="{{book.image}}" />
        <div class="image-overlay">
          <div>{{book.description}}</div>
        </div>
        <h5>{{book.name}}</h5>
        <p>By:
          <span>{{book.author}}</span>
        </p>
      </a>
    {{/each}}
  </div>
</div>
<div class="center-align">
  <a href="/signup">
    <button
      class="btn waves-effect waves-light pulse"
      type="submit"
      name="action"
    >
      <i>Get Started</i>
    </button>
  </a>
</div>