<div class="row">
  <div class="col s12 m6 offset-m3">
    <div class="card">
      <div class="card-content">

        <form>
          <div class="row">
            <div class="input-field col s12">
              <input placeholder="Search..." id="firstname" type="text" class="validate">
              <label for="firstname" class="active">Book Title:</label>
            </div>
          </div>
          <div class="row">
            <div class="input-field col s12">
              <p>
                <label>
                  <input type="checkbox" class="filled-in"/>
                  <span>Read</span>
                </label>
              </p>
              <p>
                <label>
                  <input type="checkbox" class="filled-in"/>
                  <span>Owned</span>
                </label>
              </p>
              <p>
                <label>
                  <input type="checkbox" class="filled-in"/>
                  <span>Wishlist</span>
                </label>
              </p>
              <p>
                <div class="input-field col s6">
                  <input placeholder="How did you hear about this book?" id="heard_about" type="text" class="validate">
                </div>
              </p>
            </div>
          </div>
        </form>
      </div>
      <div class="card-action center" id="div" class="row center">
        <a class="waves-effect waves-light btn-large #26a69a">Search</a>
      </div>
    </div>
  </div>
</div>
</div>
</div>
<script type="text/javascript" src="js/materialize.min.js"></script>


