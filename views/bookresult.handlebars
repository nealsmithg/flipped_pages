<script type="text/javascript">
  google.books.load(); function initialize() { var viewer = new
  google.books.DefaultViewer(document.getElementById('viewerCanvas')); var index
  = document.URL.indexOf('?'); if (index > 0) { var bookIsbn =
  document.URL.substring(index, document.URL.length).split("=")[1]
  console.log(bookIsbn); } //viewer.load('ISBN:0738531367'); //for test purpose
  viewer.load('ISBN:'+bookIsbn); } google.books.setOnLoadCallback(initialize);
</script>

<body>
  <div class="container justify-content-center">
    <div
      class="text-center"
      id="viewerCanvas"
      style="width: 1000px; height: 700px"
    ></div>
  </div>
